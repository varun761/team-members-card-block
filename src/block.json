{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wedevoop/team-members-card-block",
	"version": "0.1.0",
	"title": "Team Members Card Block",
	"category": "widgets",
	"icon": "flag",
	"description": "A block to display team members.",
	"attributes": {
		"image": {
			"type": "string",
			"default": ""
		},
		"name": {
			"type": "string",
			"source": "html",
			"selector": ".name"
		},
		"designation": {
			"type": "string",
			"source": "html",
			"selector": ".designation"
		},
		"year_of_experience": {
			"type": "string"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "team-members-card-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
